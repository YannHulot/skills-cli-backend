{
  "name": "skills-cli-backend",
  "version": "1.0.0",
  "description": "This si the backend for a hypothetical CLI based linkedin/job board",
  "main": "index.js",
  "scripts": {
    "build": "prisma generate tsc",
    "start": "tsc && node dist/index.js",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "check-types": "tsc --pretty --noEmit --skipLibCheck",
    "lint:fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "check-format": "prettier --check .",
    "check-lint": "eslint 'src/**/*.ts'",
    "prepare": "husky install",
    "seed": "prisma db seed --preview-feature",
    "test": "TEST=true jest -i",
    "test:watch": "TEST=true jest --watch",
    "postgres:start": "docker-compose up -d",
    "db:push": "prisma migrate reset --force --preview-feature && prisma migrate deploy --preview-feature",
    "migrate:reset": "prisma migrate reset",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "jest-mock-extended": "^2.0.1",
    "nodemon": "^2.0.12",
    "prettier": "2.4.0",
    "prisma": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "@types/hapi-pino": "8.0.2",
    "@types/hapi__hapi": "20.0.9",
    "@types/jsonwebtoken": "8.5.5"
  },
  "dependencies": {
    "@hapi/boom": "9.1.4",
    "@hapi/hapi": "20.1.5",
    "joi": "17.4.2",
    "@sendgrid/mail": "7.4.6",
    "date-fns": "2.23.0",
    "hapi-auth-jwt2": "10.2.0",
    "hapi-pino": "8.3.0",
    "jsonwebtoken": "8.5.1",
    "@okta/jwt-verifier": "^2.3.0",
    "@prisma/client": "^3.0.2",
    "dotenv": "^10.0.0"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}